<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.CooperationMapper">

    <insert id="addCooperation" parameterType="Cooperation">
        insert into cooperation(name,phone,email,introduce,state) values(#{name},#{phone},#{email},#{introduce},0)
    </insert>
</mapper>